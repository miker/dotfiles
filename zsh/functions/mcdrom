local mounted
local cpwd
mounted=$(grep cdrom /etc/mtab)
eject -t
if [[ $mounted = "" ]];then
    mount /cdrom
    echo "-- mounted cdrom --"
    cd /cdrom ; ls
    else
    cpwd=$(pwd|grep cdrom)
    if [[ $cpwd = "" ]];then
        umount /cdrom
        echo "-- umounted cdrom --"
        else
        cd;umount /cdrom
        echo "-- umounted cdrom --"
        pwd
        eject
    fi
fi

# vim: set et fenc=utf-8 ff=unix sts=4 sw=4 ts=4 tw=80 syn=zsh :
